{
  "name": "@ionic/react",
  "version": "0.0.6",
  "description": "React specific wrapper for @ionic/core",
  "keywords": [
    "ionic",
    "framework",
    "react",
    "mobile",
    "app",
    "hybrid",
    "webapp",
    "cordova",
    "progressive web app",
    "pwa"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "deploy": "np --any-branch",
    "tsc": "tsc -p .",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@trust/webcrypto": "^0.9.2",
    "@types/jest": "^23.3.9",
    "@types/node": "10.12.9",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "np": "^3.1.0",
    "react-testing-library": "^7.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "@ionic/core": "^4.5.0-dev.201905311531.e014c6a",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.01",
    "react-router-dom": "^5.01"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
